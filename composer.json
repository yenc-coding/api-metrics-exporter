{
    "name": "sidcorp-resource/api-metrics-exporter",
    "description": "Laravel 9+ API metrics exporter for Prometheus and Grafana dashboards",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "metrics",
        "prometheus",
        "grafana",
        "monitoring",
        "api"
    ],
    "authors": [
        {
            "name": "SidCorp Resource",
            "email": "chuongld@sidcorp.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-redis": "*",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/http": "^9.0|^10.0|^11.0|^12.0",
        "psr/log": "^1.0|^2.0|^3.0",
        "psr/http-server-middleware": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^9.5|^10.0|^11.0",
        "mockery/mockery": "^1.5"
    },
    "suggest": {
        "ext-redis": "Required for Redis storage driver"
    },
    "autoload": {
        "psr-4": {
            "SidcorpResource\\ApiMetricsExporter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SidcorpResource\\ApiMetricsExporter\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SidcorpResource\\ApiMetricsExporter\\ApiMetricsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}
